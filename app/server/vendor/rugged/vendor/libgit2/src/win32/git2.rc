      VALUE "ProductName",	"libgit2\0"
      VALUE "ProductVersion",	LIBGIT2_VERSION "\0"
      VALUE "Comments",		"For more information visit http://libgit2.github.com/\0"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1252
  END
