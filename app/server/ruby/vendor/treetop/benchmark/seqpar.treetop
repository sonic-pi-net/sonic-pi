grammar SeqPar
 
  rule statement
      'par ' (statement ' ')+ 'end'
    / 'sequence' ' ' (statement ' ')+ 'end'
    / 'seq' ' ' (statement ' ')+ 'end'
    / ('fit' [\s] (statement ' ')+ 'end') {
      def foo
        "foo"
      end
    }
    / 'art'+ [ ] (statement ' ')+ 'end'
    / [A-Z] [a-zA-z0-9]*
  end
 
end